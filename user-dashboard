	function ecwid_getUrlParam(url, paramName) {
		var url = url.split("#");
		url = url[0];
		var query = url.split("?");
		if (query.length > 1) {
			var params = query[1].split("&");
			for (var i = 0; i < params.length; i++) {
				var paramPair = params[i].split("=");
				if (paramPair[0] == paramName) {
					return paramPair.length > 1 ? paramPair[1] : "";
				}
			}
		}
		return undefined;
	}

	function ecwid_addUrlParam(url, paramPair) {
		if (url.indexOf("?") != -1) {
			url = url.replace("?", "?" + paramPair + "&");
		} else if (url.indexOf("#") != -1) {
			url = url.replace("#", "?" + paramPair + "#");
		} else {
			url += "?" + paramPair;
		}
		return url;
	}

	function ecwid_doRedirect(url) {
		// Preserve inline parameter if it exists in current query string
		var inlineParam = ecwid_getUrlParam(window.location.href, "inline");
		if (inlineParam != undefined) {
			url = ecwid_addUrlParam(url, "inline=" + inlineParam);
		}
		if (window.console) {
			window.console.log("doRedirect to url: " + url);
		}
		location.replace(url);
		document.body.style.display = "none";
		setTimeout(function() {
			location.reload(true);
		}, 300);
	}

	function ecwid_showElement(el) {
		if (el.style.removeProperty) {
			el.style.removeProperty("display");
		} else {
			el.style.removeAttribute("display");
		}
	}

	function ecwid_exchangeSrcAttr(el) {
		el.src = el.getAttribute("data-src");
	}

	function ecwid_initLoader() {
		if (ecwid_use_new_loader) {
			ecwid_showElement(document.getElementById("loader"));
		} else {
			ecwid_exchangeSrcAttr(document.getElementById("oldLoader"));
			ecwid_showElement(document.getElementById("loader-old"));
		}
	}

	window.ecwidReginaCsrfToken = "c8b760dc3b9fd41cc9b0cbad233e5866";

    function ecwid_initMobileJsApi() {
		window.EcwidControlPanel = window.EcwidControlPanel || {};
		window.EcwidControlPanel.onSavePageDataRequired = function(changePageDataCallback) {
			window.EcwidControlPanel.changePageDataCallback = changePageDataCallback;
		}
		window.EcwidControlPanel.onSavePageData = function(savePageDataCallback) {
			window.EcwidControlPanel.savePageDataCallback = savePageDataCallback;
		}
		window.EcwidControlPanel.onRevertUnsavedPageData = function(revertUnsavedPageDataCallback) {
			window.EcwidControlPanel.revertUnsavedPageDataCallback = revertUnsavedPageDataCallback;
		}
    }
</script>
